                                <p>Full Schedule</p>
                            </a>
                        </div> #}
                    
                    </div>
                    <!-- end span4 -->
                    <div class="span8">
                        <!-- TIMELINE -->
                        <section class="timeline toggle-shortcode toggles">
                            {% for event in startupweekend_content('Events_Day_2') %}
                                <article class="event">
                                    <div class="timeline-icon">
                                        <div class="timeline-icon-container">
                                            <i class=""></i>
                                        </div>
                                    </div>
                                    <div class="time-box">
                                        {{ event.time }}
                                    </div>
                                    <div class="timeline-content">
                                        <div class="event-content">
                                            <div class="toggle-item-title event-title" data-count="1">
                                                {% if event.title is defined %}
                                                    <h3>{{ event.title }}</h3>
                                                {% endif %}
                                                {# this is a quickfix #}
                                                {% if event.name is defined %}
                                                    <h3>{{ event.name }}</h3>
                                                {% endif %}
                                            </div>
                                            <div class="toggle-item-body" style="display: none;">
                                                <p>
                                                    {% if event.description is defined %}
                                                        {{ event.description }}
                                                    {% endif %}
                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                </article>
                            {% endfor %}
                        </section>
                    </div>
                    <!-- end span8 -->
