{% set org = "FoundersHub" %}
{% set org_url = "http://www.startupweekendrheinmain.de/" %}
{% set title = org ~ " StartupWeekend" %}
{% set description = title ~ " is a 54 hour frenzy of business model creation, coding, designing, and market validation. Come share ideas, form teams, and launch startups." %}
{% set event_start = '2015-09-27T18:00:00+02:00' %}
{% set event_end = '2015-09-29T21:00:00+02:00' %}
{% set contact_phone = '+491776837705' %}
<!DOCTYPE html>
<html lang="{{ locale }}">
<head>
<!-- Basic Page Needs -->
<meta charset="utf-8" />
<title>{{ title }}</title>
<!-- Metas -->
<meta name="description" content="{{ description }}" />
<!-- Mobile Specific Metas -->
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<!-- Stylesheets -->
{% stylesheets
'bundles/foundershubstartupweekend/css/bootstrap.css'
'bundles/foundershubstartupweekend/css/bootstrap-responsive.css'
'bundles/foundershubstartupweekend/css/style.css'
'bundles/foundershubstartupweekend/css/fonts.css'
'bundles/foundershubstartupweekend/css/fontello.css'
'bundles/foundershubstartupweekend/css/jquery.countdown.css'
'bundles/foundershubstartupweekend/css/flexslider.css'
filter='cssrewrite,?uglifycss' %}
<link rel="stylesheet" href="{{ asset(asset_url) }}">
{% endstylesheets %}
<!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
<!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
<![endif]-->
<!-- Favicon and touch icons -->
<link rel="apple-touch-icon" href="{{ asset("bundles/foundershubstartupweekend/images/touch-icon-iphone.png") }}">
<link rel="apple-touch-icon" sizes="76x76" href="{{ asset("bundles/foundershubstartupweekend/images/touch-icon-ipad.png") }}">
<link rel="apple-touch-icon" sizes="120x120" href="{{ asset("bundles/foundershubstartupweekend/images/touch-icon-iphone-retina.png") }}">
<link rel="apple-touch-icon" sizes="152x152" href="{{ asset("bundles/foundershubstartupweekend/images/touch-icon-ipad-retina.png") }}">
<link rel="shortcut icon" href="{{ asset("bundles/foundershubstartupweekend/images/favicon.ico") }}">
<!-- social media -->
<!-- Twitter cards support -->
<meta name="twitter:card" content="summary">
<meta name="twitter:site" content="{{ twitter }}">
<meta name="twitter:title" content="{{ title }}">
<meta name="twitter:description" content="{{ description }}">
<meta name="twitter:creator" content="coderbyheart">
{% image '@FoundershubStartupWeekendBundle/Resources/public/images/logo.png' %}
<meta name="twitter:image:src" content="{{ app.request.uriForPath(asset_url) }}">
{% endimage %}
<!-- Facebook open graph support -->
<meta property="og:title" content="{{ title }}">
<meta property="og:description" content="{{ description }}">
<meta property="og:site_name" content="{{ title }}">
{% image '@FoundershubStartupWeekendBundle/Resources/public/images/logo.png' %}
<meta property="og:image" content="{{ app.request.uriForPath(asset_url) }}">
{% endimage %}
<meta property="og:type" content="website">
</head>

<body>
<header class="top-bar" id="topbar">
  <div class="container">
    <div class="row">
      <div class="span12"><!-- logo link --><a class="logo pull-left" href="#intro" title="{{ title }}"><span></span></a>
        <div class="navbar main-nav pull-right">
          <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse"> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> </button>
          <nav>
            <div class="nav-collapse collapse">
              <ul id="mainnav" class="nav">
                <li> <a href="#about">About</a> </li>
                {% if coaches|default([])|length > 0 %}<li> <a href="#speakers">Speakers</a> </li>{% endif %}
                {# <li><a href="#schedule">Schedule</a> </li> #}
                {% if workshops|default([])|length > 0 %}<li><a href="#workshops">Workshops</a> </li>{% endif %}
                <li><a href="#venue">Venue</a> </li>
                {% if sponsors|default([])|length > 0 %}<li><a href="#sponsors">Sponsors</a> </li>{% endif %}
                {% if news|default([])|length > 0 %}<li><a href="#news">News</a> </li>{% endif %}
                <li><a href="#contact">Contact</a> </li>
              </ul>
            </div>
          </nav>
        </div>
        <!-- end navbar -->
      </div>
    </div>
    <!-- end row -->
  </div>
  <!-- end container -->
</header>
<!-- end top-bar -->
<section id="intro">
  <div class="container">
    <div class="flexslider">
      <ul class="slides">
        <li>
          <div class="row jumbotron">
            <div class="span12 text-center">
              <h1>{{ title }}</h1>
              <h3 class="info">
                  <span class="days">{{ event_start|date('d') }}&ndash;{{ event_end|date('d') }}</span>
                  <span class="month"><b>{{ event_start|date('M') }}</b></span><span class="year">{{ event_start|date('Y') }}</span>
                  <span class="location">Frankfurt</span>
              </h3>
              <a href="#newsletter" class="btn btn-large">Get tickets</a> </div>
          </div>
        </li>
      </ul>
    </div>
  </div>
  <!-- end container -->
</section>
<!-- end intro section -->
<section id="about">
  <div class="container">
    <div class="row">
      <div class="span12">
        <div class="module-header about-header">
          <h4>About</h4>
        </div>
        <!-- end module-header -->
      </div>
      <div class="span12 hero-unit text-center">
        <h1>Launch your startup in 54 hours</h1>
        <h3>
          <time datetime="{{ event_start|date('Y-m-d') }}">{{ event_start|date('d') }}</time>
          -
          <time datetime="{{ event_end|date('Y-m-d') }}">{{ event_end|date('d') }}</time>
            {{ event_start|date('M Y') }}, Frankfurt</h3>
        <h4>{{ description }}</h4>
      </div>
      <!-- end hero-unit -->
      <div class="span12">
        <div class="divider-space"></div>
      </div>
      <div class="span4 text-center">
        <div class="icon-wrap large color1"><i class="iconf-lightbulb"></i> </div>
        <h3>Pitch your idea</h3>
        <p>Pitch your idea in front of an international audience. Make it count because only the 10 best ideas will be executed during the weekend.</p>
      </div>
      <div class="span4 text-center">
        <div class="icon-wrap large color2"><i class="iconf-beaker"></i> </div>
        <h3>Build your startup</h3>
        <p>Meet the people that will help you bring your idea to life or join a team that fascinates you and work with them during the whole weekend.</p>
      </div>
      <div class="span4 text-center">
        <div class="icon-wrap large color3"><i class="iconf-star"></i> </div>
        <h3>Present your product</h3>
        <p>On Sunday your idea will be judged by a panel of international entrepreneurs and startup experts. The winning teams will receive exciting prices and continued support by the judges!</p>
      </div>
    </div>
    <!-- end row -->
  </div>
  <!--end container-->
</section>
<section id="register">
  <div class="container">
    <div class="row">
      <div id="countdown"></div>
      <div class="span12 white register-box text-center">
        <h2 class="register-title">Tickets Available Soon!</h2>
        <a href="#newsletter" class="btn btn-large btn-primary">Notify me!</a>
      </div>
    </div>
    <!-- end row -->
  </div>
  <!-- end container -->
</section>
{% include "FoundershubStartupWeekendBundle:partials:index/coaches.html.twig" %}
{# include "FoundershubStartupWeekendBundle:partials:index/schedule.html.twig" #}
