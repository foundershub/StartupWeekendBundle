  <div class="container">
    <div class="row">
      <div class="span12">
        <div class="module-header contact-header">
          <h4>Imprint</h4>
        </div>
      </div>
      <!-- end span12 -->
      <div class="span12 hero-unit text-center white">
        <h2>Get in touch</h2>
        <p>We'd love to hear from you!</p>
      </div>
      <!-- end hero-unit -->
        <div class="span12 text-center">
            <div class="social">
                {#<a href="#" target="_blank" title="Facebook" class="icon-wrap small facebook"> <i
                            class="iconf-facebook"></i> </a>#}
