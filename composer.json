{
    "name": "foundershub/foundershub-startupweekend-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle containing the FoundersHub RheinMain StartupWeekend public website.",
    "keywords": ["symfony2", "bundle"],
    "homepage": "http://github.com/foundershub/StartupWeekendBundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "alamshahi",
            "homepage": "http://themeforest.net/user/alamshahi",
            "role": "Theme Developer"
        },
        {
            "name": "Markus Tacker",
            "email": "m@coderbyheart.com",
            "homepage": "https://coderbyheart.com/",
            "role": "Developer"
        }
    ],

    "require": {
        "php": ">=5.3.9",
        "symfony/framework-bundle": "~2.3",
        "abraham/twitteroauth": "0.2.0.3",
        "dothiv/dothiv-parsedown-bundle": ">=1.0.0",
        "dothiv/dothiv-valueobject-bundle": "1.x",
        "dothiv/dothiv-contentful-bundle": ">=0.2.1",
        "twig/extensions": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },

    "config": {
        "bin-dir": "bin"
    },

    "autoload": {
        "psr-0": {
            "Foundershub\\Bundle\\StartupWeekendBundle": ""
        }
    },

    "target-dir": "Foundershub/Bundle/StartupWeekendBundle",

    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "abraham/twitteroauth",
                "type": "package",
                "version": "0.2.0.3",
                "dist": {
                    "url": "https://github.com/abraham/twitteroauth/archive/0.2.0-beta3.zip",
                    "type": "zip"
                }
            }
        }
    ]

}
